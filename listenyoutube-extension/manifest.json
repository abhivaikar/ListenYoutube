{
  "name": "Youtube in an extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Play youtube videos in Chrome extension",
  "homepage_url": "http://extensionizr.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "http://m.youtube.com/*"
  ]
}